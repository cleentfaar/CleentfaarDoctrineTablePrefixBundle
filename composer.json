{
    "name": "cleentfaar/doctrine-table-prefix-bundle",
    "type": "symfony-bundle",
    "description": "This bundle allows doctrine to recognize any prefix you might use on your table names (e.g. acme_)",
    "keywords": ["doctrine","symfony"],
    "license": "MIT",
    "require": {
        "php": ">=5.3.0",
        "symfony/framework-bundle": ">=2.0,<2.3-dev",
        "symfony/yaml": "~2.3",
        "doctrine/orm": ">=2.2.3,<2.4-dev",
        "doctrine/doctrine-bundle": "~1.2"
    },
    "autoload": {
        "psr-0": { "Cleentfaar\\DoctrineTablePrefixBundle": "" }
    },
    "target-dir": "Cleentfaar/DoctrineTablePrefixBundle"
}
